    dict_key_2: secret
    dict_list_1:
    - sublist_key_a: secret
    - sublist_key_b: secret
  list:
  - list_key_1: secret
  - list_key_2: secret
  - list_list_1:
    - sublist_key_a: secret
    - sublist_key_b: secret
  - list_dictionary:
      subdict_key_1: secret
      subdict_key_2: secret
  multiline_chevron: secret
  multiline_pipe: secret
  unsave_tag: secret

# mapping to vaulted_variables:
multiline_chevron: '{{ vault_multiline_chevron }}'
